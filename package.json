{
  "name": "lets-make-someone-happy-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "docker-build": "docker build . -t lcsl",
    "docker-start": "docker run --name lcsl -p 3100:3100 -d lcsl",
    "docker-stop": "docker stop lcsl && docker rm -f lcsl || exit 0;",
    "lint": "eslint --ext .js --ignore-path .gitignore .",
    "lint:fix": "eslint --ext .js --fix --ignore-path .gitignore ."
  },
  "dependencies": {
    "cookie-parser": "1.4.4",
    "debug": "2.6.9",
    "dotenv": "8.2.0",
    "express": "4.16.1",
    "http-errors": "1.6.3",
    "jade": "1.11.0",
    "morgan": "1.9.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/eslint-parser": "7.18.2",
    "@babel/plugin-proposal-private-methods": "7.12.1",
    "@babel/preset-env": "7.12.11",
    "babel-eslint": "10.1.0",
    "babel-preset-es2020": "1.0.2",
    "eslint": "7.32.0"
  },
  "engines": {
    "node": ">= 16.x.x"
  }
}
