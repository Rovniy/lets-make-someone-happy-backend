{
  "name": "lets-make-someone-happy-backend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "docker-build": "docker build -t lcsl .",
    "docker-pg": "docker run --name lcsl-pg -p 5433:5432 -e POSTGRES_USER=local -e POSTGRES_PASSWORD=local -e POSTGRES_DB=local -d postgres:14.4",
    "docker-start": "docker run --name lcsl -p 3000:3000 -d lcsl",
    "docker-stop": "docker stop lcsl && docker rm -f lcsl || exit 0;",
    "lint": "eslint --ext .js --ignore-path .gitignore .",
    "lint:fix": "eslint --ext .js --fix --ignore-path .gitignore .",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback --all",
    "unlock": "knex migrate:unlock",
    "drop": "npm run rollback && npm run migrate && npm run seed",
    "seed": "knex seed:run"
  },
  "imports": {
    "#root/*": "./*.js",
    "#utils/*": "./utils/*.js",
    "#helpers": "./utils/helpers.js",
    "#db": "./db/index.js",
    "#types": "./utils/types.js",
    "#constants": "./utils/constants.js"
  },
  "dependencies": {
    "cookie-parser": "1.4.4",
    "debug": "2.6.9",
    "dotenv": "8.2.0",
    "express": "4.16.1",
    "http-errors": "1.6.3",
    "knex": "2.1.0",
    "moment": "^2.29.4",
    "morgan": "1.9.1",
    "objection": "3.0.1",
    "pg": "8.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/eslint-parser": "7.18.2",
    "@babel/plugin-proposal-private-methods": "7.12.1",
    "@babel/preset-env": "7.12.11",
    "@faker-js/faker": "^7.3.0",
    "babel-eslint": "10.1.0",
    "babel-preset-es2020": "1.0.2",
    "eslint": "7.32.0"
  },
  "engines": {
    "node": ">= 16.x.x"
  }
}
